- rates = item.shipping_rates
- if rates.nil? or rates.is_a?(String)
  Error: #{rates || 'No Fetch'}
- else
  = fields_for "OrderItem[#{item.id}]", item do |i|
    != i.collection_select(:shipping_id, rates, :id, :description_price, { :include_blank => true })
  - if @script
    %script!= "window.merge_listing(#{item.normal_h.to_json});"
